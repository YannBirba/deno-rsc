{
  "imports": {
    "esbuild": "https://esm.sh/v111/esbuild@0.17.11",
    "react": "https://esm.sh/v111/react@18.2.0",
    "react-dom": "https://esm.sh/v111/react-dom@18.2.0",
    "react-dom/client": "https://esm.sh/v111/react-dom@18.2.0/client"
  },
  "types": [
    "https://deno.land/x/aleph@1.0.0-beta.36/types.d.ts"
  ],
  "tasks": {
    "install:node": "cd esbuild-entrypoint && pnpm i",
    "serve": "deno run --watch --allow-net --allow-read --allow-ffi --allow-run --allow-env server.ts"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "https://esm.sh/v111/react@18.2.0"
  }
}