{
  "importMap": "./import_map.json",
  "types": [
    "https://deno.land/x/aleph@1.0.0-beta.36/types.d.ts"
  ],
  "tasks": {
    "install-node": "cd node-root && npm i --legacy-peer-deps",
    "cache-reload": "deno cache --reload server.ts",
    "serve": "deno run --watch --allow-net --allow-read --allow-ffi --allow-run --allow-env server.dev.ts"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "dom.extras",
      "deno.ns"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "https://esm.sh/v111/react@18.2.0"
  }
}